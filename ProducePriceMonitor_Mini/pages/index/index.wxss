/**index.wxss**/
.clear{
  clear: both;
  overflow: hidden;
}
navigator{
  display:inline;
}
.SearchBar{
  position: fixed;
}
.list{
  margin-top:10px;
}
 
.list .list_item{
  margin-top:10px;
  padding:10px;
  height:100px;
  border-bottom:1px solid #E8E8E8;
}
.list .list_item .img{
  float:left;
  width:30%;
  height:100%;
}
.list .list_item .img image{
   width:100%;
   height:100%;
}
 
.list .list_item .info{
  width:69%;
  float:right;
  height:100px;
  position:relative;
  flex-direction: column ;
}
.list .list_item .info .h_info{
  width:100%;
  display: flex;
  flex-direction: row ;
  align-items: left;
}
.list .list_item .info .h_info .v_info{
  width:80%;
  float:right;
  display: flex;
  flex-direction: column ;
  align-items: left;
}
.list .list_item .info .h_info .v_info .commit{
  width:100%;
  display: flex;
  flex-direction: row;
  align-items: left;
}
.list .list_item .info .title{
  color:#333;
  margin-left:10px;
  font-size: 15px;
  overflow:hidden;
  text-overflow: ellipsis;
  display:-webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
 
.list .list_item .info .h_info .v_info .price{
  color:#FF2727;
  margin-left:10px;
  margin-top:2px;
  font-size:15px;
}
.list .list_item .info .h_info .v_info .commit .commitcount{
  color:#333;
  margin-left:10px;
  margin-top:2px;
  font-size:12px;
}
.list .list_item .info .h_info .v_info .commit .goodcommit{
  color:#333;
  margin-left:10px;
  margin-top:2px;
  font-size:12px;
}
.list .list_item .info .h_info .v_info .store{
  color:#333;
  margin-left:10px;
  margin-top:2px;
  font-size:12px;
  align-items: left;
}
.list .list_item .info .h_info .btn[plain]{
  border:0;
  width: 20%;
  height: 100%;
}
.list .list_item .info .h_info .btn .image{
  margin: 0;
  width:24rpx;
  height: 24rpx;
  border: none;
 }